{
  "spinnerStream": "stdout",
  "progressStream": "stdout",
  "reasonerStream": "stderr",
  "options": [
    {
      "mode": "bundle",
      "project": "tsconfig.lib.json",
      "exportFilename": "index.ts",
      "useSemicolon": true,
      "useBanner": false,
      "useTimestamp": false,
      "quote": "\"",
      "directive": "",
      "fileExt": "none",
      "overwrite": true,
      "backup": false,
      "generationStyle": "default-alias-named-destructive",
      "include": [
        "src/lib/**/*.js",
        "src/lib/**/*.jsx",
        "src/lib/**/*.ts",
        "src/lib/**/*.tsx"
      ],
      "exclude": [
        "dist/**",
        "src/**/*.stories.@(js|jsx|ts|tsx|mdx)",
        "src/**/*.spec.@(js|jsx|ts|tsx)",
        "src/.storybook/**/*"
      ],
      "output": "src/",
      "removeBackup": true,
      "forceYes": false
    }
  ]
}