{
  "name": "@acme/shared-ui",
  "version": "0.0.1",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "nx": {
    "name": "shared-ui",
    "projectType": "library",
    "tags": [
      "type:ui",
      "scope:shared"
    ],
    "targets": {
      "generate:barrel": {
        "executor": "nx:run-commands",
        "options": {
          "command": "ctix build",
          "cwd": "{projectRoot}"
        }
      }
    }
  }
}
